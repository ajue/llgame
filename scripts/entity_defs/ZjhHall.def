<root>
	<Implements>
	</Implements>

	<Properties>
		<hallID>
			<Type>			INT32				</Type>
			<Flags>			BASE				</Flags>
		</hallID>

	</Properties>

	<BaseMethods>

		<!-- 客户端调用该接口请求进入大厅 -->
		<reqEnterHall>
			<!-- 玩家的mailbox -->
			<Arg>	MAILBOX	</Arg>
		</reqEnterHall>

		<!-- 客户端调用该接口请求离开大厅 -->
		<reqLeaveHall>
			<!-- 玩家的mailbox -->
			<Arg>	MAILBOX	</Arg>
		</reqLeaveHall>

		<!-- 客户端调用该接口请求进入大厅房间/桌子 -->
		<reqEnterRoom>
			<!-- 玩家的mailbox -->
			<Arg>	MAILBOX	</Arg>

			<!-- 房间的ID -->
			<Arg>	INT32	</Arg>
		</reqEnterRoom>

		<!-- 客户端调用该接口请求离开房间 -->
		<reqLeaveRoom>
			<!-- 玩家的mailbox -->
			<Arg>	MAILBOX	</Arg>

			<!-- 房间的ID -->
			<Arg>	INT32	</Arg>
		</reqLeaveRoom>

		<!-- 获取玩家数量 -->
		<reqPlayerCount>
		</reqPlayerCount>

		<reqMessage>
		<Arg>	MAILBOX	</Arg>
		<Arg>	INT32	</Arg>
        <Arg>	UNICODE	</Arg>
		</reqMessage>
	</BaseMethods>

	<CellMethods>
	</CellMethods>
</root>

